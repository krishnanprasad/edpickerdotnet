    <script>
        var IndexPageApp = angular.module("myApp", []);
        IndexPageApp.controller("IndexPageCtrl", ['$scope', '$http', '$rootScope', function ($scope, $http, $rootScope) {
            $scope.CallAlert = function (name) {
                var post = $http({
                    method: "POST",
                    url: "/GetLocation",
                    dataType: 'json',
                    data: { Filter: name },
                    headers: { "Content-Type": "application/json" }
                });

                post.then(function (data, status) {


                    $rootScope.Search = data.data;

                });
            }



            $scope.FireSearchTB = function (name) {
                var post = $http({
                    method: "POST",
                    url: "/EdPicker/GetTags",
                    dataType: 'json',
                    data: { q: name },
                    headers: { "Content-Type": "application/json" }
                });

                post.then(function (data, status) {


                    $rootScope.SearchResultTags = data.data;

                });
            }
            @{
                var IndexTrending = new System.Web.Script.Serialization.JavaScriptSerializer();
                var IndexTrendingjson = IndexTrending.Serialize(Model.TrendingList);

            }

            $rootScope.Trending =@Html.Raw(IndexTrendingjson);

             @{
                var NewsTrending = new System.Web.Script.Serialization.JavaScriptSerializer();
                var NewsTrendingjson = NewsTrending.Serialize(Model.NewsListForIndexPage);

            }

            $rootScope.NewsList =@Html.Raw(NewsTrendingjson);

         @{
                var BlogTrending = new System.Web.Script.Serialization.JavaScriptSerializer();
                var BlogTrendingjson = BlogTrending.Serialize(Model.BlogListForIndexPage);

            }

            $rootScope.BlogList =@Html.Raw(BlogTrendingjson);

             @{
                var CalenderTrending = new System.Web.Script.Serialization.JavaScriptSerializer();
                var CalenderTrendingjson = CalenderTrending.Serialize(Model.CalenderListForIndexPage);

            }

            $rootScope.CalenderList =@Html.Raw(CalenderTrendingjson);


            $scope.ChangeLocation = function (SearchLocation) {
                var post = $http({
                    method: "POST",
                    url: "/EdPicker/GetLocationTrial",
                    dataType: 'json',
                    data: { Location: SearchLocation },
                    headers: { "Content-Type": "application/json" }
                });

                post.then(function (data, status) {

                 
                    $rootScope.LocationResult = data.data;

                });
            }
            $scope.SaveLocation = function (ResultLocation) {


                $scope.LocationSelected=ResultLocation;
            }

            $scope.GoToSearchPage = function ()
            {

                var Location = document.getElementById('LocationID').innerHTML;
                var TagSelected = document.getElementById('TagSelected').innerHTML;
                if(Location=="Enter location here"&& TagSelected=="Enter what you want to learn here..")
                {
                    alert("Please Tell Us What you want to Learn and Location");
                }
                else
                {
                    window.location.href="/Edpicker/Search?q="+TagSelected+'&Location='+Location;
                }
            }
            $scope.GoToQuickSearch = function (Direct)
            {

                var Location = document.getElementById('LocationID').innerHTML;
                var TagSelected = Direct;
                window.location.href="/Edpicker/Search?q="+TagSelected+'&Location='+Location;
            }
            $scope.QuickSearch = function (Direct,Location)
            {

                var TagSelected = Direct;
                window.location.href="/EdPicker/Search?q="+TagSelected+'&Location='+Location;
            }
            $scope.TagSelect= function (Search)
            {
                $scope.TagSelected=Search;
            }
            $scope.CallPage=function(InstitutionID)
            {
                window.location.href ="/EdPicker/GoToPage?InstitutionId="+InstitutionID;
            }
            $scope.ClickforSelectLocation = function (CityName) {
               
                $scope.Location = CityName;
             
            }
            $scope.GoToPage = function (controller,action) {

                window.location.href = "/" + controller + "/" + action;

            }
            $scope.GoToReferencePage = function (sNO, URLLink, mOdule) {
                var post = $http({
                    method: "POST",
                    url: "/EdPicker/AddCount",
                    dataType: 'json',
                    data: { SNo: sNO, Module: mOdule },
                    headers: { "Content-Type": "application/json" }
                });

                post.then(function (data, status) {

                    window.location.href = URLLink;
                
                });
            }
        }]);
    </script>
