    <script>
        var SearchPageApp = angular.module("SearchPage", []);


        SearchPageApp.controller("SearchResult",function ($scope, $http, $rootScope) {
            $scope.SaveRegister = function () {
               
                var institutionName = document.getElementById('institname').value;
                var location = document.getElementById('institilocation').value;
                var courseOffered = document.getElementById('institcourse').value;
                var contactName = document.getElementById('institcontactname').value;
                var contactNumber = document.getElementById('institcontactnos').value;
                document.getElementById('registerbutn').style.visibility = 'hidden';
                var post = $http({
                
                    method: "POST",
                    url: "/SaveRegistration",
                    dataType: 'json',
                    data: {InstitutionName: institutionName,Location: location, CourseOffered: courseOffered, ContactName:contactName, ContactNumber:contactNumber },
                    headers: { "Content-Type": "application/json" }
                });

                post.then(function (data, status) {

                    alert("Thanks for Registration")
                    window.location.href = "/Index/";

                });
            }


        });

    </script>

